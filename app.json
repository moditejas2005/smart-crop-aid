{
  "expo": {
    "name": "smart_crop_aid",
    "slug": "smart_crop_aid",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_icon.png",
    "scheme": "smartcropaid",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/app_icon.png"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.tejas_modi.smart_crop_aid"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#32CD32",
          "dark": {
            "backgroundColor": "#2E7D32"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "This app needs access to location to provide weather updates and location-based crop recommendations."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "This app needs access to your photos to upload crop images for pest detection.",
          "cameraPermission": "This app needs access to your camera to take photos of crops for pest detection."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "41f9d208-65a3-4a81-b0df-e34e524c2da2"
      }
    }
  }
}